
 import "./order.zmodel"  

model Product {

    id                String        @id @default(uuid())  
    name            String?          
    description  String?          
    category    String?          

orders        Order[]      @relation("product")  
      dateCreated DateTime @default(now())
      dateUpdated DateTime @updatedAt  @default(now())
      @@allow("all", true)
}